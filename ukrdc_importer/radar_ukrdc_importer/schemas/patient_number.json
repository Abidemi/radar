{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "number": {
            "type": ["string", "integer"]
        },
        "number_type": {
            "type": "string"
        },
        "organization": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                }
            }
        }
    },
    "required": ["number", "number_type", "organization"]
}
