{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "external_id": {
            "type": "string"
        },
        "from_time": {
            "type": "string",
            "format": "date-time"
        },
        "result": {
            "type": "object",
            "properties": {
                "result_items": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "observation_time": {
                                "type": "string",
                                "format": "date-time"
                            },
                            "test_item_code": {
                                "type": "object",
                                "properties": {
                                    "code": {
                                        "type": "string"
                                    },
                                    "description": {
                                        "type": "string"
                                    }
                                },
                                "required": ["code", "description"]
                            },
                            "result_value": {
                                "type": "string",
                                "format": "number"
                            },
                            "required": ["observation_time", "test_item_code", "result_value"]
                        }
                    }
                }
            },
            "required": ["result_items"]
        },
        "entering_organization": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                }
            },
            "required": ["code", "description"]
        }
    },
    "required": ["external_id", "result", "entering_organization"]
}
