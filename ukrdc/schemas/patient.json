{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "name": {
            "type": "object",
            "properties": {
                "given_name": {
                    "type": "string"
                },
                "family_name": {
                    "type": "string"
                }
            }
        },
        "birth_time": {
            "type": "string"
        },
        "death_time": {
            "type": "string"
        },
        "gender": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                }
            },
            "required": ["code", "description"]
        },
        "ethnic_group": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                }
            },
            "required": ["code", "description"]
        },
        "contact_info": {
            "type": "object",
            "properties": {
                "home_phone_number": {
                    "type": "string"
                },
                "work_phone_number": {
                    "type": "string"
                },
                "mobile_phone_number": {
                    "type": "string"
                },
                "email_address": {
                    "type": "string"
                }
            }
        }
    }
}
